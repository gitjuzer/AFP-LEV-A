{
  "name": "gitjuzer/afpleva-backend",
  "description": "REST API az AFP-LEV-A egyetemi projekthez",
  "keywords": [
    "micro framework",
    "rest",
    "router",
    "psr7"
  ],
  "homepage": "http://github.com/gitjuzer/AFP-LEV-A",
  "license": "MIT",
  "authors": [
    {
      "name": "EKE PTI-BSC-L-2017"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "slim/slim": "^3.1",
    "tuupola/slim-jwt-auth": "^3.1",
    "fzaninotto/faker": "^1.8"
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.8 < 6.0"
  },
  "autoload": {
    "psr-4": {
      "Afp\\": "src/",
      "Afp\\Api\\": "src/1_api",
      "Afp\\Domain\\": "src/2_domain",
      "Afp\\Repository\\": "src/3_repository",
      "Afp\\Data\\": "src/4_data",
      "Afp\\Shared\\": "src/0_shared"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 0
  },
  "scripts": {
    "start": "php -S localhost:4999 -t public",
    "test": "phpunit"
  }
}
